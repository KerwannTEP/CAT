\begin_body

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
T & =\int_{r_{p}}^{r_{a}}\frac{2dr}{\sqrt{2(\psi(r)-E)-\frac{L^{2}}{r^{2}}}}=\int_{r_{p}}^{r_{a}}\frac{2rdr}{\sqrt{2r^{2}(\psi(r)-E)-L^{2}}}\\
 & \stackrel[Y(x)=2r^{2}\psi(r)]{x=2r^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{Y(x)-Ex-L^{2}}}\\
 & =\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{Y(x)-(Ex-L_{c}^{2})+(L_{c}^{2}-L^{2})}}\\
 & \stackrel{\ell^{2}=L_{c}^{2}-L^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{\ell^{2}-\left((Ex-L_{c}^{2})-Y(x)\right)}}\\
 & \stackrel{z^{2}(x)=(Ex-L_{c}^{2})-Y(x)}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{\ell^{2}-z^{2}(x)}}\\
 & \stackrel[f(z(x))=x]{x=f(z)}{=}\frac{1}{2}\int_{-\ell}^{\ell}\frac{f'(z)dz}{\sqrt{\ell^{2}-z^{2}}}\\
 & \stackrel{z=\ell\sin\phi}{=}\frac{1}{2}\int_{-\pi/2}^{\pi/2}f'(\ell\sin\phi)d\phi
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
z^{2}(x)=Y(x)+(Ex-L_{c}^{2})
\]

\end_inset


\begin_inset Formula 
\[
z^{2}=z^{2}(f(z))=Y(f(z))+(Ef(z)-L_{c}^{2})
\]

\end_inset


\begin_inset Formula 
\[
z^{2}=Y(f(z))+(Ef(z)-L_{c}^{2})
\]

\end_inset


\begin_inset Formula 
\[
2z=f'(z)Y'(f(z))+Ef'(z)
\]

\end_inset


\begin_inset Formula 
\[
2z=f'(z)\left[Y'(f(z))+E\right]
\]

\end_inset


\begin_inset Formula 
\[
\frac{2z}{Y'(f(z))+E}=f'(z)
\]

\end_inset

with 
\begin_inset Formula 
\[
f(z)=x=2r^{2}
\]

\end_inset


\begin_inset Formula 
\[
Y(x)=2r^{2}\psi(r)=x\psi(\sqrt{x/2})=\frac{x}{\sqrt{1+x/2}}
\]

\end_inset


\begin_inset Formula 
\[
Y'(x)=\frac{x+4}{\sqrt{2}(x+2)^{3/2}}>0
\]

\end_inset

.
 with 
\begin_inset Formula $z^{2}=Y(f(z))+(Ef(z)-L_{c}^{2})$
\end_inset

 we have
\begin_inset Formula 
\[
f'(z)=\frac{2z}{Y'(f(z))+E}
\]

\end_inset


\begin_inset Formula 
\[
f'(\ell\sin\phi)=\frac{2\ell\sin\phi}{Y'(f(\ell\sin\phi))+E}
\]

\end_inset

with 
\begin_inset Formula 
\[
\ell^{2}=L_{c}^{2}-L^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
z^{2}(x) & =Y(x)+(Ex-L_{c}^{2})\\
 & =x(\psi(\sqrt{x/2})+E)
\end{align*}

\end_inset


