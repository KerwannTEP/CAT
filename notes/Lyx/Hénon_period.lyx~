\begin_inset Formula 
\begin{align*}
\Theta & =\int_{r_{p}}^{r_{a}}\frac{2Ldr}{r^{2}\sqrt{2(\psi(r)-E)-\frac{L^{2}}{r^{2}}}}=\int_{r_{p}}^{r_{a}}\frac{2rdr}{r^{2}\sqrt{2r^{2}(\psi(r)-E)-L^{2}}}\\
 & \stackrel[Y(x)=2r^{2}\psi(r)]{x=2r^{2}}{=}\int_{x_{p}}^{x_{a}}\frac{dx}{x\sqrt{Y(x)-Ex-L^{2}}}\\
 & =\int_{x_{p}}^{x_{a}}\frac{dx}{x\sqrt{Y(x)-(Ex-L_{c}^{2})+(L_{c}^{2}-L^{2})}}\\
 & \stackrel{\ell^{2}=L_{c}^{2}-L^{2}}{=}\int_{x_{p}}^{x_{a}}\frac{dx}{x\sqrt{\ell^{2}-(Y(x)+(Ex-L_{c}^{2}))}}\\
 & \stackrel{z^{2}(x)=Y(x)+(Ex-L_{c}^{2})}{=}\int_{x_{p}}^{x_{a}}\frac{dx}{x\sqrt{\ell^{2}-z^{2}(x)}}\\
 & \stackrel{x=f(z)}{=}\int_{-\ell}^{\ell}\frac{f'(z)dz}{f(z)\sqrt{\ell^{2}-z^{2}}}\\
 & \stackrel{z=\ell\sin\phi}{=}\int_{-\pi/2}^{\pi/2}\frac{f'(\ell\sin\phi)}{f(\ell\sin\phi)}d\phi
\end{align*}

\end_inset


