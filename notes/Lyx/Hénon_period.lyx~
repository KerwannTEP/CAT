\begin_layout Standard
Résumé:
\begin_inset Formula 
\begin{align*}
T & =\frac{1}{2}\int_{-\pi/2}^{\pi/2}f'(\ell\sin\phi)d\phi
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\Theta & =\int_{-\pi/2}^{\pi/2}\frac{f'(\ell\sin\phi)}{f(\ell\sin\phi)}d\phi
\end{align*}

\end_inset


\begin_inset Formula 
\[
f:z\in[-\ell,\ell]\mapsto x\in[x_{p},x_{a}]
\]

\end_inset


\begin_inset Formula 
\[
f(0)=x_{c}
\]

\end_inset


\begin_inset Formula 
\[
f=z^{-1}
\]

\end_inset


\begin_inset Formula 
\[
\phi(x)=\frac{x}{\sqrt{1+x/2}}+Ex-L_{c}^{2}
\]

\end_inset


\begin_inset Formula 
\[
f'(z)=\begin{cases}
-\frac{2|z|}{\phi'(x)} & x\in[x_{a},x_{c}[\leftrightarrow z\in[-\ell,0[\\
+\frac{2|z|}{\phi'(x)} & x\in]x_{c},x_{p}]\leftrightarrow z\in]0,\ell]\\
\sqrt{\frac{2}{\phi''(x_{c})}} & x=x_{c}\leftrightarrow z=0\leftrightarrow\phi'(x)=0
\end{cases}
\]

\end_inset

with the bijectiion 
\begin_inset Formula 
\[
z(x)=\begin{cases}
-\sqrt{\phi(x)} & x\in[x_{a},x_{c}]\\
+\sqrt{\phi(x)} & x\in[x_{c},x_{p}]
\end{cases}
\]

\end_inset

with 
\begin_inset Formula $x_{c}=2r_{c}^{2}$
\end_inset

 the 
\begin_inset Quotes eld
\end_inset

circular radius
\begin_inset Quotes erd
\end_inset

.
 Equivalently, can be found when 
\begin_inset Formula $\psi_{{\rm eff}}'(r_{c};L)=0$
\end_inset

.
\end_layout

