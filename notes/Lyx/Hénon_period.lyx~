\begin_layout Standard
\begin_inset Formula 
\[
\langle D\rangle=\int_{r_{p}}^{r_{a}}D(r)\frac{dr}{v_{r}}=\frac{1}{4}\int_{-\pi/2}^{\pi/2}D(r)f'(\ell\sin\phi)d\phi
\]

\end_inset


\begin_inset Formula 
\[
x=2r^{2}
\]

\end_inset


\begin_inset Formula 
\[
z^{2}(x)=(Ex-L_{c}^{2})-Y(x)
\]

\end_inset


\begin_inset Formula 
\[
x=f(z)
\]

\end_inset


\begin_inset Formula 
\[
z=\ell\sin\phi
\]

\end_inset

donc
\begin_inset Formula 
\[
x=f(\ell\sin\phi)
\]

\end_inset

donc
\begin_inset Formula 
\[
f(\ell\sin\phi)=2r^{2}
\]

\end_inset

donc
\begin_inset Formula 
\[
r=\sqrt{\frac{f(\ell\sin\phi)}{2}}
\]

\end_inset


\end_layout

