\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\langle D\rangle=\int_{r_{p}}^{r_{a}}D(r)\frac{dr}{v_{r}}=\frac{1}{4}\int_{-\pi/2}^{\pi/2}D(r)f'(\ell\sin\phi)d\phi
\]

\end_inset


\begin_inset Formula 
\[
x=2r^{2}
\]

\end_inset


\begin_inset Formula 
\[
z^{2}(x)=(Ex-L_{c}^{2})-Y(x)
\]

\end_inset


\begin_inset Formula 
\[
x=f(z)
\]

\end_inset


\begin_inset Formula 
\[
z=\ell\sin\phi
\]

\end_inset

donc
\begin_inset Formula 
\[
x=f(\ell\sin\phi)
\]

\end_inset

donc
\begin_inset Formula 
\[
f(\ell\sin\phi)=2r^{2}
\]

\end_inset

donc
\begin_inset Formula 
\[
r=\sqrt{\frac{f(\ell\sin\phi)}{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
Résumé:
\begin_inset Formula 
\begin{align*}
T & =\frac{1}{2}\int_{-\pi/2}^{\pi/2}f'(\ell\sin\phi)d\phi
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\Theta & =\int_{-\pi/2}^{\pi/2}\frac{f'(\ell\sin\phi)}{f(\ell\sin\phi)}d\phi
\end{align*}

\end_inset


\begin_inset Formula 
\[
f:z\in[-\ell,\ell]\mapsto x\in[x_{p},x_{a}]
\]

\end_inset


\begin_inset Formula 
\[
f(0)=x_{c}
\]

\end_inset


\begin_inset Formula 
\[
f=z^{-1}
\]

\end_inset


\begin_inset Formula 
\[
\phi(x)=\frac{x}{\sqrt{1+x/2}}+Ex-L_{c}^{2}
\]

\end_inset


\begin_inset Formula 
\[
f'(z)=\begin{cases}
\frac{2z}{\phi'(x)} & x\neq x_{c}\leftrightarrow z\neq0\\
\sqrt{\frac{2}{\phi''(x_{c})}} & x=x_{c}\leftrightarrow\phi'(x)=0\leftrightarrow z=0
\end{cases}
\]

\end_inset

with the bijectiion 
\begin_inset Formula 
\[
z(x)=\begin{cases}
-\sqrt{\phi(x)} & x\in[x_{a},x_{c}]\\
+\sqrt{\phi(x)} & x\in[x_{c},x_{p}]
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
z'(x)=\begin{cases}
-\frac{\phi'(x)}{2\sqrt{\phi(x)}}=\frac{\phi'(x)}{2z(x)} & x\in[x_{a},x_{c}]\\
+\frac{\phi'(x)}{2\sqrt{\phi(x)}}=\frac{\phi'(x)}{2z(x)} & x\in[x_{c},x_{p}]
\end{cases}
\]

\end_inset


\begin_inset Formula $z$
\end_inset

 continue sur 
\begin_inset Formula $[x_{p},x_{a}]$
\end_inset

, derivable sur l'intervalle privé de 
\begin_inset Formula $x_{c}$
\end_inset

, et limite finie en 
\begin_inset Formula $x_{c}$
\end_inset

.
 donc 
\begin_inset Formula $C^{1}$
\end_inset

 sur 
\begin_inset Formula $[x_{p},x_{a}]$
\end_inset

.
 dérivée toujours non-nulle (et positive).
 donc inverse est 
\begin_inset Formula $C^{1}$
\end_inset

 de dérivée strict positive.
 (inversion globale)
\end_layout

\begin_layout Standard
with 
\begin_inset Formula $x_{c}=2r_{c}^{2}$
\end_inset

 the 
\begin_inset Quotes eld
\end_inset

circular radius
\begin_inset Quotes erd
\end_inset

.
 Equivalently, can be found when 
\begin_inset Formula $\psi_{{\rm eff}}'(r_{c};L)=0$
\end_inset

.
\end_layout

