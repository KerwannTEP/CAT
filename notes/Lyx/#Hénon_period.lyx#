\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
E=\psi(r)+v^{2}/2=\psi(r)+\dot{r}^{2}/2+L^{2}/2r
\]

\end_inset


\begin_inset Formula 
\[
E\geq\psi(r)\geq-1
\]

\end_inset


\begin_inset Formula 
\[
-E\leq1
\]

\end_inset


\begin_inset Formula 
\begin{align*}
T & =\int_{r_{p}}^{r_{a}}\frac{2dr}{\sqrt{2(E-\psi(r))-\frac{L^{2}}{r^{2}}}}=\int_{r_{p}}^{r_{a}}\frac{2rdr}{\sqrt{2r^{2}(E-\psi(r))-L^{2}}}\\
 & \stackrel[Y(x)=2r^{2}\psi(r)]{x=2r^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{Ex-Y(x)-L^{2}}}\\
 & =\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{-Y(x)+(Ex-L_{c}^{2})+(L_{c}^{2}-L^{2})}}\\
 & \stackrel{\ell^{2}=L_{c}^{2}-L^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{\ell^{2}-\left(Y(x)-(Ex-L_{c}^{2})\right)}}\\
 & \stackrel{z^{2}(x)=Y(x)-(Ex-L_{c}^{2})}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{dx}{\sqrt{\ell^{2}-z^{2}(x)}}\\
 & \stackrel[\varphi(z(x))=x]{x=\varphi(z)}{=}\frac{1}{2}\int_{-\ell}^{\ell}\frac{\varphi'(z)dz}{\sqrt{\ell^{2}-z^{2}}}\\
 & \stackrel{z=\ell\sin\phi}{=}\frac{1}{2}\int_{-\pi/2}^{\pi/2}\varphi'(\ell\sin\phi)d\phi
\end{align*}

\end_inset


\begin_inset Formula 
\[
x=2r^{2}
\]

\end_inset


\begin_inset Formula 
\[
z^{2}(x)=Y(x)-(Ex-L_{c}^{2})\leftrightarrow x=\varphi(z)
\]

\end_inset


\begin_inset Formula 
\[
z=\ell\sin\phi
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\langle D\rangle & =\int_{r_{p}}^{r_{a}}D(r)\frac{2dr}{\sqrt{2(E-\psi(r))-\frac{L^{2}}{r^{2}}}}=\int_{r_{p}}^{r_{a}}D(r)\frac{2rdr}{\sqrt{2r^{2}(E-\psi(r))-L^{2}}}\\
 & \stackrel[Y(x)=2r^{2}\psi(r)]{x=2r^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{D(r)dx}{\sqrt{Ex-Y(x)-L^{2}}}\\
 & =\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{D(r)dx}{\sqrt{-Y(x)+(Ex-L_{c}^{2})+(L_{c}^{2}-L^{2})}}\\
 & \stackrel{\ell^{2}=L_{c}^{2}-L^{2}}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{D(r)dx}{\sqrt{\ell^{2}-\left(Y(x)-(Ex-L_{c}^{2})\right)}}\\
 & \stackrel{z^{2}(x)=Y(x)-(Ex-L_{c}^{2})}{=}\frac{1}{2}\int_{x_{p}}^{x_{a}}\frac{D(r)dx}{\sqrt{\ell^{2}-z^{2}(x)}}\\
 & \stackrel[\varphi(z(x))=x]{x=\varphi(z)}{=}\frac{1}{2}\int_{-\ell}^{\ell}\frac{D(r)\varphi'(z)dz}{\sqrt{\ell^{2}-z^{2}}}\\
 & \stackrel{z=\ell\sin\phi}{=}\frac{1}{2}\int_{-\pi/2}^{\pi/2}D(r)\varphi'(\ell\sin\phi)d\phi
\end{align*}

\end_inset


\end_layout

