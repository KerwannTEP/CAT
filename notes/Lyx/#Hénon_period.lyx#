\begin_inset Formula 
\[
z^{2}=Y(x)-(Ex-L_{c}^{2})=-\frac{x}{\sqrt{1+x/2}}-Ex+L_{c}^{2}=\phi(x)\leftrightarrow\begin{cases}
z=\sqrt{\phi(x)} & x>x_{{\rm c}}\\
z=-\sqrt{\phi(x)} & x<x_{{\rm c}}
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
z^{2}=\phi(f(z))\rightarrow2z=f'(z)\phi'(f(z))\rightarrow f'(z)=\frac{2z}{\phi'(f(z))}
\]

\end_inset


\begin_inset Formula 
\[
\phi'(x)=-\frac{4+x}{\sqrt{2}(2+x)^{3/2}}-E
\]

\end_inset


\begin_inset Formula 
\[
\phi''(x)=\frac{x+8}{2\sqrt{2}(2+x)^{5/2}}>0
\]

\end_inset

donc 
\begin_inset Formula $\phi'(x)$
\end_inset

 croissant.
 Pour 
\begin_inset Formula $E>-1\leftrightarrow-E<1$
\end_inset


\begin_inset Formula 
\[
\phi'(0)=-1-E<0
\]

\end_inset


\begin_inset Formula 
\[
\phi'(x)=-E>0
\]

\end_inset

donc 
\begin_inset Formula $\phi(x)$
\end_inset

 décroissant sur 
\begin_inset Formula $[0,x_{c}]$
\end_inset

 puis croissant sur 
\begin_inset Formula $[x_{c},\infty]$
\end_inset

.
 
\begin_inset Formula 
\[
\phi(0)=L_{c}^{2}
\]

\end_inset


\begin_inset Formula 
\[
\phi(x_{c})=0
\]

\end_inset


\begin_inset Formula 
\[
\phi(+\infty)=+\infty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $x=\varphi(z)$
\end_inset

 solution de 
\begin_inset Formula $z^{2}=\phi(x)$
\end_inset

 with 
\begin_inset Formula $x>0$
\end_inset

.
 Two solutions in 
\begin_inset Formula $[0,x_{c}]$
\end_inset

 and 
\begin_inset Formula $[x_{c},\infty]$
\end_inset

.
 By the previous convention, exists and well-defined:
\begin_inset Formula 
\[
z<0\leftrightarrow x<x_{c}
\]

\end_inset


\begin_inset Formula 
\[
z>0\leftrightarrow x>x_{c}
\]

\end_inset

We also have 
\begin_inset Formula $f'(z)=\frac{2z}{\phi'(f(z))}$
\end_inset

 with 
\begin_inset Formula $\phi'(x<x_{c})<0$
\end_inset

 and 
\begin_inset Formula $\phi(x>x_{c})>0$
\end_inset

, therefore 
\begin_inset Formula $f'(z)>0$
\end_inset

 for 
\begin_inset Formula $z\neq0$
\end_inset

.
 As for 
\begin_inset Formula $z=0$
\end_inset

, we have 
\begin_inset Formula 
\begin{align*}
z(x_{c}+\epsilon) & =\sqrt{z^{2}(x_{c}+\epsilon)}=\sqrt{z_{c}^{2}+\epsilon\phi'(x_{c})+\frac{\epsilon^{2}}{2}\phi''(x_{c})+o(\epsilon^{2})}\\
 & =\epsilon\sqrt{\frac{\phi''(x_{c})}{2}}+o(\epsilon)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{z(x_{c}+\epsilon)-z(x_{c})}{\epsilon}=\frac{z(x_{c}+\epsilon)}{\epsilon}=\sqrt{\frac{\phi''(x_{c})}{2}}+o(1)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
z(x_{c}-\epsilon) & =-\sqrt{z^{2}(x_{c}-\epsilon)}=\sqrt{z_{c}^{2}-\epsilon\phi'(x_{c})+\frac{\epsilon^{2}}{2}\phi''(x_{c})+o(\epsilon^{2})}\\
 & =-\epsilon\sqrt{\frac{\phi''(x_{c})}{2}}+o(\epsilon)
\end{align*}

\end_inset


\begin_inset Formula 
\[
\frac{z(x_{c}-\epsilon)-z(x_{c})}{-\epsilon}=\frac{z(x_{c}-\epsilon)}{-\epsilon}=\sqrt{\frac{\phi''(x_{c})}{2}}+o(1)
\]

\end_inset

hence 
\begin_inset Formula 
\[
f'(z=0)=
\]

\end_inset

Equation can be transformed into the polynomial
\begin_inset Formula 
\[
z^{2}=Y(x)-(Ex-L_{c}^{2})=\frac{x}{\sqrt{1+x/2}}-Ex+L_{c}^{2}
\]

\end_inset


\begin_inset Formula 
\[
z^{2}-L_{c}^{2}=(\frac{-1}{\sqrt{1+x/2}}-E)x\rightarrow\frac{z^{2}-L_{c}^{2}}{x}+E=\frac{-1}{\sqrt{1+x/2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{z^{2}-L_{c}^{2}+Ex}{x}=\frac{-1}{\sqrt{1+x/2}}\rightarrow\frac{\left(z^{2}-L_{c}^{2}+Ex\right)^{2}}{x^{2}}=\frac{1}{1+x/2}
\]

\end_inset

i.e.
 
\begin_inset Formula $x=\varphi(z)$
\end_inset

 solution de 
\begin_inset Formula 
\[
\left(z^{2}-L_{c}^{2}+Ex\right)^{2}\left(1+x/2\right)=x^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
degree-3 polynomial in 
\begin_inset Formula $x$
\end_inset

.
 by previous analysis: two solution in 
\begin_inset Formula $[0,x_{c}]$
\end_inset

 and 
\begin_inset Formula $[x_{c},\infty]$
\end_inset

 (more precisely, in 
\begin_inset Formula $[x_{p},x_{c}]$
\end_inset

 and 
\begin_inset Formula $[x_{c},x_{a}]$
\end_inset

).
 One more negative solution in the degree3 polynomial, to be discarded.
 Ordering of the roots:
\begin_inset Formula 
\[
x_{{\rm neg}}<x_{1}<x_{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
there is a solution in 
\begin_inset Formula $[x_{\max},\infty[$
\end_inset

, another in 
\begin_inset Formula $[0,x_{\max}[$
\end_inset

 and another in 
\begin_inset Formula $]-\infty,-\frac{1}{2}[$
\end_inset

.
 => Positive discriminant: order the roots and select the middle one =>
 
\begin_inset Formula $x=\varphi(z)$
\end_inset

 is semi-analytical (analytical if we are given 
\begin_inset Formula $L_{c}$
\end_inset

).then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
T & (E,L)=\frac{1}{2}\int_{-\pi/2}^{\pi/2}f'(\ell\sin\phi)d\phi
\end{align*}

\end_inset

then 
\begin_inset Formula 
\[
\frac{\partial T}{\partial E}(E,L)=\frac{1}{2}\frac{\partial\ell}{\partial E}\int_{-\pi/2}^{\pi/2}\sin\phi f''(\ell\sin\phi)d\phi
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial T}{\partial L}(E,L)=\frac{1}{2}\frac{\partial\ell}{\partial L}\int_{-\pi/2}^{\pi/2}\sin\phi f''(\ell\sin\phi)d\phi
\]

\end_inset

with 
\begin_inset Formula 
\[
\ell\frac{\partial\ell}{\partial E}(E,L)=L_{c}(E)\frac{\partial L_{c}}{\partial E}(E)
\]

\end_inset


\begin_inset Formula 
\[
\ell\frac{\partial\ell}{\partial L}(E,L)=-L
\]

\end_inset

donc
\begin_inset Formula 
\[
\frac{\partial\ell}{\partial E}(E,L)=\frac{L_{c}(E,L)}{\ell(E,L)}\frac{\partial L_{c}}{\partial E}(E)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial\ell}{\partial L}(E,L)=-\frac{L}{\ell(E,L)}
\]

\end_inset


